# This is a synthetic SNG test file

IHDR {
	width 16
	height 15
	bitdepth 8
	using color palette 
	with interlace
}

gAMA {4.5}

# This compiles, but triggers a libpng warning about invalid SBIT depth
# The parameters are the standard values in 4.2.2.3
#cHRM {
#   white (31270, 32900)
#   red   (6400,  3300)
#   green (3000,  6000)
#   blue  (1500,  600)
#}

# This cannot coexist with the iCCP chunk.
# sRGB {1}	# This value conveys `relative colorimetric' intent.

# This cannot coexist with the sRGB chunk.
iCCP {"dummy profile"}

sBIT {
  red 8
  green 8
  blue 8
  # gray 8	# for non-color images
  # alpha 8	# for images with alpha
}

PLTE {
   (0,     0, 255)
   (255,   0,   0)
   ( 70,  70,  70)
}

bKGD {
  # red 127
  # green 127
  # blue 127
  # gray 127	# for non-color images
  index 0	# for paletted images
}

hIST {23, 55, 0}

# tRNS processing doesn't seem to work
#tRNS {
#   10 10 10
   # red 70 
   # green 70
   # blue 70
#}

pHYs {
   resolution (500, 400) per meter
}

IMAGE {
   base64
0000001111100000
0000011111110000
0000111001111000
0001110000111100
0001110000111100
0000110001111000
0000000011110000
0000000111100000
0000001111000000
0000001111000000
0000000000000000
0000000110000000
0000001111000000
0000001111000000
0000000110000000
}

tEXt {				# Ordinary text chunk
  keyword "Title"
  text "Sample SNG script"
}

zTXt {				# Compressed text chunk
  keyword "Author"
  text "Eric S. Raymond"
}

iTXt {				# International UTF-8 keyword
  language "en"
  keyword "Description"
  text "SNG demo, illustrating the language syntax"
}
